        }
    ],
    "scripts": {
        "tests" : "phpunit --bootstrap bootstrap.php",
        "up:release": "gh release create \"v1.0.5\" --generate-notes"
    },
    "keywords": [
        "client"
    ],
    "require": {
        "php": ">=7.4.0",
        "symfony/dotenv": "^5.2",
        "guzzlehttp/guzzle": "~6.5.5",
        "symfony/event-dispatcher": "^5.4",
        "illuminate/container": "^8.83",
        "symfony/routing": "^5.4",
        "symfony/http-kernel": "^5.4",
        "illuminate/http": "^8.83",
        "symfony/validator": "^5.4",
        "illuminate/routing": "^8.83",
        "illuminate/translation": "^8.83",
        "illuminate/validation": "^8.83",
        "illuminate/cookie": "^8.83",
        "illuminate/testing": "^8.83"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "9.*",
        "mockery/mockery": "^1.4",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "AppM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
